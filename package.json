{
  "name": "testcursor",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development next dev -p 3000",
    "build": "NODE_ENV=production next build",
    "start": "NODE_ENV=production next start",
    "lint": "next lint",
    "test": "jest --config jest.config.ts",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:coverage:watch": "jest --coverage --watch",
    "test:coverage:report": "jest --coverage && open coverage/lcov-report/index.html",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "test:verify-ci": "act -j test",
    "migrate:tests": "ts-node src/utils/scripts/migrate-to-colocation.ts",
    "migrate:tests:dry": "ts-node src/utils/scripts/migrate-to-colocation.ts --dry-run",
    "migration:report": "ts-node src/utils/scripts/test-migration-report.ts",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "screenshot": "node scripts/screenshot.mjs",
    "type-check": "tsc --noEmit",
    "prepare": "husky",
    "security-scan": "npm audit && snyk test",
    "performance": "lighthouse http://localhost:3000 --output json --output-path ./lighthouse-report.json",
    "pre-commit": "npm run lint && npm run type-check && npm test",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:playwright": "playwright test",
    "test:e2e:mock": "playwright test --grep 'ポイントシステム E2Eテスト'",
    "test:e2e:user": "playwright test --grep 'ポイントシステム 実ユーザーフロー'",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:ui": "playwright test --ui",
    "playwright:install": "playwright install --with-deps",
    "playwright:report": "playwright show-report",
    "test:unit": "jest --testPathPattern=.unit.test.ts",
    "test:integration": "jest --testPathPattern=.integration.test.tsx",
    "test:cypress": "cypress run",
    "generate:test": "ts-node src/utils/scripts/generate-test.ts"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@supabase/supabase-js": "^2.49.4",
    "@types/pg": "^8.11.11",
    "@types/react-beautiful-dnd": "^13.1.8",
    "critters": "^0.0.25",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "framer-motion": "^12.6.3",
    "html2canvas": "^1.4.1",
    "next": "14.1.0",
    "pg": "^8.14.1",
    "puppeteer": "^24.6.0",
    "react": "^18",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^18",
    "react-icons": "^5.5.0",
    "recharts": "^2.15.2",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.6",
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@playwright/test": "^1.51.1",
    "@storybook/addon-essentials": "^8.6.12",
    "@storybook/addon-onboarding": "^8.6.12",
    "@storybook/blocks": "^8.6.12",
    "@storybook/experimental-addon-test": "^8.6.12",
    "@storybook/experimental-nextjs-vite": "^8.6.12",
    "@storybook/react": "^8.6.12",
    "@storybook/test": "^8.6.12",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/node-fetch": "^2.6.12",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/testing-library__jest-dom": "^6.0.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.29.0",
    "@typescript-eslint/parser": "^8.29.0",
    "@vitest/browser": "^3.1.1",
    "@vitest/coverage-v8": "^3.1.1",
    "autoprefixer": "^10.4.21",
    "chalk": "^5.3.0",
    "cypress": "^14.3.0",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "eslint-plugin-storybook": "^0.12.0",
    "glob": "^10.3.10",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "ignore-loader": "^0.1.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lighthouse": "^12.5.1",
    "node-fetch": "^2.7.0",
    "playwright": "^1.51.1",
    "postcss": "^8.5.3",
    "snyk": "^1.1296.1",
    "start-server-and-test": "^2.0.11",
    "storybook": "^8.6.12",
    "tailwindcss": "^3.4.17",
    "ts-jest": "^29.3.1",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "vitest": "^3.1.1"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
