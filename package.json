{
  "name": "testcursor",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development next dev -p 3000",
    "build": "NODE_ENV=production next build",
    "start": "NODE_ENV=production next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:unit": "jest --testPathPattern=.unit.test.ts",
    "test:integration": "JEST_INTEGRATION_TEST=true jest --testPathPattern=.integration.test.ts --maxWorkers=2",
    "test:coverage": "jest --coverage --maxWorkers=2 --testPathIgnorePatterns=tests/integration --testPathIgnorePatterns=tests/e2e",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:ui": "playwright test --ui",
    "test:accessibility": "playwright test tests/e2e/accessibility.e2e.test.ts",
    "test:smoke": "playwright test tests/e2e/release-smoke-test.e2e.test.ts",
    "test:supabase": "source ./set_env.sh && node scripts/mcp-supabase-integration-test.js",
    "test:supabase:debug": "source ./set_env.sh && DEBUG_SUPABASE=true node scripts/mcp-supabase-integration-test.js",
    "test:ci": "npm run lint && npm run type-check && npm run test:unit && npm run test:supabase && npm run test:e2e",
    "test:pre-release": "npm run test:smoke && npm run test:accessibility",
    "pre-commit": "npm run lint && npm run type-check && npm run test",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "playwright:install": "playwright install --with-deps",
    "playwright:report": "playwright show-report",
    "security-scan": "npm audit && snyk test",
    "performance": "lighthouse http://localhost:3000 --output json --output-path ./lighthouse-report.json",
    "prepare": "husky",
    "env:production": "cp .env.production .env.local && echo '🚀 本番環境に切り替えました'",
    "env:staging": "cp .env.staging .env.local && echo '🧪 Staging環境に切り替えました'",
    "env:local": "cp .env.example .env.local && echo '🖥️ ローカル環境に切り替えました'",
    "env:backup": "cp .env.local .env.backup && echo '💾 現在の設定をバックアップしました'",
    "deploy:staging": "node scripts/deploy.js deploy staging",
    "deploy:production": "node scripts/deploy.js deploy production",
    "deploy:rollback:staging": "node scripts/deploy.js rollback staging",
    "deploy:rollback:production": "node scripts/deploy.js rollback production",
    "deploy:logs:staging": "node scripts/deploy.js logs staging",
    "deploy:logs:production": "node scripts/deploy.js logs production",
    "deploy:status:staging": "node scripts/deploy.js status staging",
    "deploy:status:production": "node scripts/deploy.js status production",
    "deploy:check": "node scripts/deploy.js check",
    "docker:up": "docker-compose up -d postgres supabase-rest",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:reset": "docker-compose down -v && docker-compose up -d postgres supabase-rest",
    "check:dev": "node scripts/env-check.js dev",
    "check:staging": "node scripts/env-check.js staging",
    "check:production": "node scripts/env-check.js production",
    "check:all": "node scripts/env-check.js all"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@hello-pangea/dnd": "^18.0.1",
    "@heroicons/react": "^2.2.0",
    "@modelcontextprotocol/sdk": "^1.11.0",
    "@mui/icons-material": "^7.0.2",
    "@mui/material": "^7.0.2",
    "@smithery/sdk": "^1.2.2",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.49.4",
    "@tailwindcss/typography": "^0.5.16",
    "@tanstack/react-query": "^5.74.0",
    "@tanstack/react-virtual": "^3.13.6",
    "@types/pg": "^8.11.11",
    "@types/react-beautiful-dnd": "^13.1.8",
    "@types/react-window": "^1.8.8",
    "@uiw/react-markdown-preview": "^5.1.4",
    "@uiw/react-md-editor": "^4.0.7",
    "clsx": "^2.1.1",
    "critters": "^0.0.25",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "framer-motion": "^12.6.5",
    "html2canvas": "^1.4.1",
    "next": "^14.2.28",
    "pg": "^8.14.1",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.5.2",
    "react-icons": "^5.5.0",
    "react-window": "^1.8.11",
    "recharts": "^2.15.2",
    "tailwind-merge": "^3.2.0",
    "use-debounce": "^10.0.4",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.1",
    "@chromatic-com/storybook": "^3.2.6",
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@playwright/test": "^1.51.1",
    "@storybook/addon-essentials": "^8.6.12",
    "@storybook/addon-onboarding": "^8.6.12",
    "@storybook/blocks": "^8.6.12",
    "@storybook/experimental-addon-test": "^8.6.12",
    "@storybook/experimental-nextjs-vite": "^8.6.12",
    "@storybook/react": "^8.6.12",
    "@storybook/test": "^8.6.12",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.14",
    "@types/jest-axe": "^3.5.9",
    "@types/node": "^20",
    "@types/node-fetch": "^2.6.12",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/testing-library__jest-dom": "^6.0.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.29.0",
    "@typescript-eslint/parser": "^8.29.0",
    "@vitest/browser": "^3.1.1",
    "@vitest/coverage-v8": "^3.1.1",
    "autoprefixer": "^10.4.21",
    "axe-playwright": "^2.1.0",
    "chalk": "^5.3.0",
    "cypress": "^14.3.0",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "eslint-plugin-storybook": "^0.12.0",
    "glob": "^10.3.10",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "ignore-loader": "^0.1.2",
    "jest": "^29.7.0",
    "jest-axe": "^10.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "lighthouse": "^12.5.1",
    "node-fetch": "^2.7.0",
    "playwright": "^1.51.1",
    "postcss": "^8.5.3",
    "puppeteer": "^24.9.0",
    "snyk": "^1.1296.1",
    "start-server-and-test": "^2.0.11",
    "storybook": "^8.6.12",
    "tailwindcss": "^3.4.17",
    "ts-jest": "^29.3.1",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "vitest": "^3.1.1"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
