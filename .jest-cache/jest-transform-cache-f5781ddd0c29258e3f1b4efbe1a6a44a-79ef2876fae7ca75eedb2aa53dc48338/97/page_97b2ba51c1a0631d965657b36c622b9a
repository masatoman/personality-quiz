                                className: "mb-4",
                                children: [
                                    /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                                        className: "flex items-center mb-2",
                                        children: [
                                            /*#__PURE__*/ (0, _jsxruntime.jsx)("span", {
                                                className: "mr-3",
                                                children: "評価："
                                            }),
                                            /*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
                                                className: "flex",
                                                children: [
                                                    1,
                                                    2,
                                                    3,
                                                    4,
                                                    5
                                                ].map((star)=>/*#__PURE__*/ (0, _jsxruntime.jsx)("button", {
                                                        type: "button",
                                                        onClick: ()=>setUserRating(star),
                                                        className: `text-2xl focus:outline-none ${userRating >= star ? "text-yellow-500" : "text-gray-300"}`,
                                                        children: "★"
                                                    }, star))
                                            })
                                        ]
                                    }),
                                    /*#__PURE__*/ (0, _jsxruntime.jsx)("textarea", {
                                        value: comment,
                                        onChange: (e)=>setComment(e.target.value),
                                        placeholder: "コメントを入力してください（任意）",
                                        className: "w-full p-3 border border-gray-300 rounded-md focus:outline-none focus:ring-2 focus:ring-blue-500",
                                        rows: 3
                                    })
                                ]
                            }),
                            /*#__PURE__*/ (0, _jsxruntime.jsx)("button", {
                                type: "submit",
                                disabled: submitting || userRating === 0,
                                className: `px-4 py-2 bg-blue-600 text-white rounded ${submitting || userRating === 0 ? "opacity-50 cursor-not-allowed" : "hover:bg-blue-700"}`,
                                children: submitting ? "送信中..." : "評価を送信"
                            })
                        ]
                    }),
                    /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                        children: [
                            /*#__PURE__*/ (0, _jsxruntime.jsx)("h4", {
                                className: "font-medium mb-4",
                                children: "フィードバック一覧"
                            }),
                            feedbacks.length === 0 ? /*#__PURE__*/ (0, _jsxruntime.jsx)("p", {
                                className: "text-gray-500",
                                children: "まだフィードバックはありません。最初の評価を投稿してみませんか？"
                            }) : /*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
                                className: "space-y-6",
                                children: feedbacks.map((feedback)=>/*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
                                        className: "border-b border-gray-200 pb-6 last:border-b-0",
                                        children: /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                                            className: "flex items-start",
                                            children: [
                                                /*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
                                                    className: "w-10 h-10 rounded-full bg-gray-200 overflow-hidden mr-4",
                                                    children: /*#__PURE__*/ (0, _jsxruntime.jsx)("img", {
                                                        src: feedback.user_avatar || "/avatars/default.png",
                                                        alt: feedback.user_name,
                                                        className: "w-full h-full object-cover"
                                                    })
                                                }),
                                                /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                                                    className: "flex-1",
                                                    children: [
                                                        /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                                                            className: "flex items-center justify-between mb-1",
                                                            children: [
                                                                /*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
                                                                    className: "font-medium",
                                                                    children: feedback.user_name
                                                                }),
                                                                /*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
                                                                    className: "text-sm text-gray-500",
                                                                    children: new Date(feedback.created_at).toLocaleDateString("ja-JP")
                                                                })
                                                            ]
                                                        }),
                                                        /*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
                                                            className: "flex text-yellow-500 mb-2",
                                                            children: [
                                                                ...Array(5)
                                                            ].map((_, i)=>/*#__PURE__*/ (0, _jsxruntime.jsx)("span", {
                                                                    children: i < feedback.rating ? /*#__PURE__*/ (0, _jsxruntime.jsx)(_fa.FaStar, {}) : /*#__PURE__*/ (0, _jsxruntime.jsx)(_fa.FaRegStar, {})
                                                                }, i))
                                                        }),
                                                        /*#__PURE__*/ (0, _jsxruntime.jsx)("p", {
                                                            className: "text-gray-700",
                                                            children: feedback.comment
                                                        })
                                                    ]
                                                })
                                            ]
                                        })
                                    }, feedback.id))
                            })
                        ]
                    })
                ]
            }),
            /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                className: "bg-white rounded-lg shadow-md p-6",
                children: [
                    /*#__PURE__*/ (0, _jsxruntime.jsx)("h3", {
                        className: "text-xl font-bold mb-6",
                        children: "関連教材"
                    }),
                    /*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
                        className: "grid grid-cols-1 md:grid-cols-3 gap-6",
                        children: relatedMaterials.map((material)=>/*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
                                className: "border border-gray-200 rounded-lg p-4 hover:border-blue-300 transition-colors",
                                children: /*#__PURE__*/ (0, _jsxruntime.jsxs)(_link.default, {
                                    href: `/materials/${material.id}`,
                                    className: "block h-full",
                                    children: [
                                        /*#__PURE__*/ (0, _jsxruntime.jsx)("h4", {
                                            className: "font-medium mb-2 hover:text-blue-600",
                                            children: material.title
                                        }),
                                        /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                                            className: "flex items-center text-sm text-gray-600 mb-2",
                                            children: [
                                                /*#__PURE__*/ (0, _jsxruntime.jsx)("span", {
                                                    className: "mr-3",
                                                    children: material.author_name
                                                }),
                                                /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                                                    className: "flex text-yellow-500",
                                                    children: [
                                                        [
                                                            ...Array(5)
                                                        ].map((_, i)=>/*#__PURE__*/ (0, _jsxruntime.jsx)("span", {
                                                                className: "text-xs",
                                                                children: i < Math.floor(material.rating) ? /*#__PURE__*/ (0, _jsxruntime.jsx)(_fa.FaStar, {}) : /*#__PURE__*/ (0, _jsxruntime.jsx)(_fa.FaRegStar, {})
                                                            }, i)),
                                                        /*#__PURE__*/ (0, _jsxruntime.jsx)("span", {
                                                            className: "ml-1 text-gray-700 text-xs",
                                                            children: material.rating
                                                        })
                                                    ]
                                                })
                                            ]
                                        }),
                                        /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                                            className: "mt-2 flex gap-2",
                                            children: [
                                                /*#__PURE__*/ (0, _jsxruntime.jsx)("span", {
                                                    className: "px-2 py-1 bg-blue-100 text-blue-800 rounded text-xs",
                                                    children: material.category === "business" ? "ビジネス英語" : material.category === "communication" ? "コミュニケーション" : material.category
                                                }),
                                                /*#__PURE__*/ (0, _jsxruntime.jsx)("span", {
                                                    className: "px-2 py-1 bg-purple-100 text-purple-800 rounded text-xs",
                                                    children: material.difficulty === "beginner" ? "初級" : material.difficulty === "intermediate" ? "中級" : "上級"
                                                })
                                            ]
                                        })
                                    ]
                                })
                            }, material.id))
                    }),
                    /*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
                        className: "mt-6 text-center",
                        children: /*#__PURE__*/ (0, _jsxruntime.jsxs)(_link.default, {
                            href: "/materials",
                            className: "inline-flex items-center text-blue-600 hover:text-blue-800",
                            children: [
                                "すべての教材を見る ",
                                /*#__PURE__*/ (0, _jsxruntime.jsx)(_fa.FaChevronRight, {
                                    className: "ml-1"
                                })
                            ]
                        })
                    })
                ]
            })
        ]
    });
};
